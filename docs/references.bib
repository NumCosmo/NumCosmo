  Eid                      = {124010},
  Eprint                   = {1503.03407},
  File                     = {Agullo2015.pdf:Cosmology/Agullo2015.pdf:PDF},
  Keywords                 = {Lower dimensional models, minisuperspace models, Loop quantum gravity quantum geometry spin foams, Quantum cosmology},
  Owner                    = {sandro},
  Primaryclass             = {gr-qc},
  Timestamp                = {2015.08.21}
}

