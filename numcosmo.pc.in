prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

Name: @PACKAGE_NAME@
Description: Cosmology numerical library
Version: @PACKAGE_VERSION@
Requires: glib-2.0 >= @GLIB_REQUIRED_VERSION@
Libs: -L${libdir} -lnumcosmo @FFTW3_LIBS@ @GLIB_LIBS@ @NLOPT_LIBS@ @GSL_LIBS@ @LIBS@
Cflags: -I${includedir} @FFTW3_CFLAGS@ @GLIB_CFLAGS@ @NLOPT_CFLAGS@ @GSL_CFLAGS@
